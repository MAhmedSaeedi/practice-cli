{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
    "_version": "16.1.0",
    "compilerOptions": {
        "lib": [
            "es2021"
        ],
        "outDir": "dist",
        "module": "Node16",
        "target": "es2021",
        "noUncheckedIndexedAccess": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node16"
    },
    "include": [
        "src"
    ]
}